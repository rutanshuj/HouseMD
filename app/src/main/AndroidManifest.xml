<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.housemd">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".Profile" />
        <activity android:name=".MedicalHistory" />
        <activity android:name=".SignUp" />
        <activity
            android:name=".FamilyHistory"
            android:label="@string/title_activity_family_history"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Father" />
        <activity android:name=".Mother" />
        <activity
            android:name=".NavigationActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity
            android:name=".Symptoms"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".NearbyActivity" />
        <activity android:name=".NearbyDisease"></activity>
    </application>

</manifest>